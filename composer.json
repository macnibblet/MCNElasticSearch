{
    "name": "mcn/elastic-search",
    "description": "A module that helps integration of elastic search",
    "type": "library",
    "license": "BSD3",
    "keywords": [
        "zf2",
        "zend framework",
        "elastic search",
        "module"
    ],
    "homepage": "https://github.com/macnibblet/MCNElasticSearch",
    "authors": [
        {
            "name": "Antoine Hedgecock",
            "email": "antoine@pmg.se"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ruflin/elastica": "0.90.2.0",
        "zendframework/zendframework": "~2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7",
        "squizlabs/php_codesniffer": "dev-master",
        "satooshi/php-coveralls": "~0.6",

        "doctrine/orm": "2.4.*",
        "doctrine/mongodb-odm": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "MCNElasticSearch\\": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    },

    "provide": {
        "ext-mongo": "1.2.15"
    }
}
